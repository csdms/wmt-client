{
  "id":"waves",
  "component_parameters":[
    {
      "key":"simulation_name",
      "name":"Simulation name",
      "description":"Name of the simulation",
      "value":{
        "type":"string",
        "default":"Waves"
      }
    },
    {
      "key":"model_end_time",
      "name":"Model end time",
      "description":"(MP) Number of days over which simulation occurs",
      "value":{
        "type":"int",
        "default":10,
        "range":{
          "min":0,
          "max":2147483647
        },
        "units":"days"
      }
    },
    {
      "key":"incoming_wave_height",
      "name":"Incoming wave height",
      "description":"Height of incoming waves",
      "value":{
        "type":"float",
        "default":2.0,
        "range":{
          "min":0.0,
          "max":100.0
        },
        "units":"m"
      }
    },
    {
      "key":"incoming_wave_period",
      "name":"Incoming wave period",
      "description":"Period of incoming waves",
      "value":{
        "type":"float",
        "default":7,
        "range":{
          "min":0,
          "max":100
        },
        "units":"s"
      }
    },
    {
      "key":"highness_factor",
      "name":"Highness factor",
      "description":"Highness factor for incoming wave angles",
      "value":{
        "type":"float",
        "default":0.2,
        "range":{
          "min":0.0,
          "max":1.0
        }
      }
    },
    {
      "key":"asymmetry",
      "name":"Asymmetry",
      "description":"Asymmetry of incoming wave angles",
      "value":{
        "type":"float",
        "default":0.5,
        "range":{
          "min":0.0,
          "max":1.0
        }
      }
    },
    {
      "key":"output_format",
      "name":"Output format",
      "description":"File format for output files",
      "value":{
        "type":"choice",
        "default":"netcdf",
        "choices":[
          "netcdf",
          "vtk"
        ]
      }
    }
  ]
}